<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>

	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<!-- SQL NULL값 있는 칼럼 NULL 값 허용해주는 설정 -->
        <setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.zoody.department.vo.DepartmentVo" alias="DepartmentVo"/>
		<typeAlias type="com.kh.zoody.user.vo.UserVo" alias="UserVo"/>
		<typeAlias type="com.kh.zoody.attendance.vo.AttendanceVo" alias="AttendanceVo"/>
		<typeAlias type="com.kh.zoody.mail.vo.MailVo" alias="MailVo"/>
		<typeAlias type="com.kh.zoody.mail.vo.MailRecipientVo" alias="MailRecipientVo"/>
		<typeAlias type="com.kh.zoody.community.vo.BoardVo" alias="BoardVo"/>
		<typeAlias type="com.kh.zoody.notice.vo.NoticeVo" alias="NoticeVo"/>
		<typeAlias type="com.kh.zoody.suggestion.vo.SuggestionVo" alias="SuggestionVo"/>
		<typeAlias type="com.kh.zoody.animal.vo.AnimalVo" alias="AnimalVo"/>
		<typeAlias type="com.kh.zoody.reply.vo.ReplyVo" alias="ReplyVo"/>
		<typeAlias type="com.kh.zoody.animal.training.vo.TrainingVo" alias="TrainingVo"/>
		<typeAlias type="com.kh.zoody.project.vo.ProjectVo" alias="ProjectVo"/>
		<typeAlias type="com.kh.zoody.document.vo.DocumentVo" alias="DocumentVo"/>
		<typeAlias type="com.kh.zoody.project.vo.ProjectTodoVo" alias="ProjectTodoVo"/>
	</typeAliases>

</configuration>